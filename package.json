{
  "name": "css-style-sorter",
  "displayName": "CSS Style Sorter",
  "icon": "assets/css-style-sorter-icon.png",
  "description": "Organize your CSS Properties with just one click.",
  "version": "0.1.0",
  "publisher": "victorhmszzero",
  "author": {
    "name": "Victor VHMS",
    "email": "victorhms.vhms@gmail.com",
    "url": "https://github.com/victorhmszzero"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Formatters",
    "Programming Languages",
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/victorhmszzero/Css-Style-Sorter"
  },
  "license": "MIT",
  "keywords": [
    "CSS",
    "style",
    "sorter",
    "organizer",
    "Visual Studio Code"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "css-style-sorter.OrganizeCSS",
        "title": "Organize with CSS Style Sorter"
      },
      {
        "command": "css-style-sorter.resetSettings",
        "title": "Reset CSS Style Sorter Settings"
      }
    ],
    "keybindings": [
      {
        "command": "css-style-sorter.OrganizeCSS",
        "key": "ctrl+alt+o",
        "mac": "cmd+alt+o"
      }
    ],
    "configuration": {
      "title": "CSS Style Sorter",
      "properties": {
        "cssStyleSorter.propertiesToOrganize": {
          "type": "array",
          "default": [
            "display",
            "position",
            "top",
            "right",
            "bottom",
            "left",
            "float",
            "clear",
            "z-index",
            "gap",
            "margin",
            "margin-top",
            "margin-right",
            "margin-bottom",
            "margin-left",
            "padding",
            "padding-top",
            "padding-right",
            "padding-bottom",
            "padding-left",
            "flex",
            "flex-direction",
            "justify-content",
            "align-items",
            "align-self",
            "grid-template-columns",
            "grid-template-rows",
            "grid-column",
            "grid-row",
            "visibility",
            "opacity",
            "outline",
            "border",
            "border-top",
            "border-right",
            "border-bottom",
            "border-left",
            "border-width",
            "border-top-width",
            "border-right-width",
            "border-bottom-width",
            "border-left-width",
            "border-style",
            "border-top-style",
            "border-right-style",
            "border-bottom-style",
            "border-left-style",
            "border-color",
            "border-top-color",
            "border-right-color",
            "border-bottom-color",
            "border-left-color",
            "border-collapse",
            "border-spacing",
            "background",
            "background-color",
            "background-image",
            "background-repeat",
            "background-position",
            "width",
            "min-width",
            "max-width",
            "height",
            "min-height",
            "max-height",
            "content",
            "color",
            "font",
            "font-family",
            "font-size",
            "font-weight",
            "font-style",
            "font-variant",
            "text-shadow",
            "text-overflow",
            "text-indent",
            "text-transform",
            "text-decoration",
            "list-style",
            "line-height",
            "letter-spacing",
            "white-space",
            "word-spacing",
            "overflow",
            "overflow-x",
            "overflow-y",
            "caption-side",
            "table-layout",
            "empty-cells",
            "box-shadow",
            "transform",
            "transition",
            "animation",
            "quotes",
            "cursor"
          ],
          "description": "Specify the CSS properties to organize"
        },
        "cssStyleSorter.resetSettings": {
          "type": "boolean",
          "default": false,
          "description": "Reset CSS Style Sorter settings to default. *Needs restart after enabling."
        }
      }
    },
    "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "vscode-test"
    },
    "devDependencies": {
      "@types/vscode": "^1.85.0",
      "@types/mocha": "^10.0.6",
      "@types/node": "18.x",
      "eslint": "^8.56.0",
      "typescript": "^5.3.3",
      "@vscode/test-cli": "^0.0.4",
      "@vscode/test-electron": "^2.3.8"
    }
  },
  "bugs": {
    "url": "https://github.com/victorhmszzero/Css-Style-Sorter/issues"
  }
}
